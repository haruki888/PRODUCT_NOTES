<header class= "navbar navbar-fixed-top navbar-light sweet-navbar">
  <div class= "container">
    <%= link_to "PRODUCT_NOTES", root_path, id: "logo" %>
    <nav>
      <ul class= "nav navbar-nav navbar-right">
        <li><%= link_to "Top", root_path %></li>
        <li><%= link_to "Sign Up", signup_path %></li>
        <% if logged_in? %>
          <li><%= link_to "Users", '#' %></li>
          <li class= "dropdown">
            <a href = "#" class = "dropdown-toggle" data-toggle= "dropdown">
              Account <b class = "caret"></b>
            </a>
            <ul class = "dropdown-menu">
              <li><%= link_to "Show", current_user %></li>
              <li><%= link_to "Settings", '#' %></li>
              <li class = "divider"></li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log_in", login_path %></li>
        <% end %>
          <li><%= link_to "Log out", logout_path, data: { turbo_method: :delete} %></li>
      </ul>
    </nav>
  </div>
</header>
